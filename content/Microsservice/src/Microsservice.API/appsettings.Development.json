{
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console" ],
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "formatter": "Serilog.Formatting.Compact.CompactJsonFormatter, Serilog.Formatting.Compact"
        }
      }
    ]
  },
  "ConnectionStrings": {
    "DefaultConnection": "Server=localhost;Database=MinhaHomolog;User Id=usr;Password=123;"
  },
  "RabbitMq": {
    "HostName": "localhost",
    "UserName": "guest",
    "Password": "guest",
    "Port": "5672"
  },
  "Jaeger": {
    "Host": "localhost",
    "Port": "4317"
  },
  "RBMQ_NOME_FILA": "NomeDaFila",
  "CACHE_INSTANCE_NAME": "Microsservice",
  "CACHE_CONFIGURATION_URL": "localhost:6380",
  "CACHE_TIME_EXPIRED_CACHED": "10",
  "PROJECT_NAME": "Microsservice",
  "Microsservice_ENDPOINT": "localhost:8080"
}